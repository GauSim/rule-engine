{
    "name": "rule-engine",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "postinstall": "npm run typings",
        "pretest": "npm run build",
        "test": "NODE_ENV='test' _mocha './dist/test/index.js' -c --inline-diffs --reporter 'spec' --ui 'bdd'",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "NODE_ENV='test' istanbul --include-all-sources cover _mocha ./dist/test/index.js",
        "start": "node ./dist",
        "build": "tsc -p .",
        "clean": "find ./dist/ -type f -name '*.js' -delete && find ./dist/ -type f -name '*.js.map' -delete",
        "prewatch": "npm run clean",
        "watch": " npm run build -- --pretty -w",
        "watch:serve": "nodemon -L --exec 'npm run start' ./dist",
        "typings": "typings install"
    },
    "devDependencies": {
        "mocha": "^2.4.5",
        "typescript": "^1.8.9",
        "typings": "^0.7.9",
        "nodemon": "^1.9.1",
        "should": "^8.3.0",
        "istanbul": "^0.4.2"
    },
    "dependencies": {
        "deep-equal": "^1.0.1",
        "underscore": "^1.8.3"
    },
    "author": "simon.gausmann@gausmann-media.de",
    "license": "ISC"
}